CREATE TABLE Inmate (
    inmateid INT(11) PRIMARY KEY AUTO_INCREMENT,
    admissionno INT(11),
    userid INT(11),
    roomid INT(11),
    department VARCHAR(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci,
    course VARCHAR(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci,
    batch VARCHAR(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci,
    dob DATE,
    checkindate DATE,
    checkoutdate DATE,
    rollno INT(11)
);
CREATE TABLE Users (
    userid INT AUTO_INCREMENT PRIMARY KEY,
    email VARCHAR(30),
    name VARCHAR(20),
    password VARCHAR(20),
    usertype INT,
    userstatus INT
);
